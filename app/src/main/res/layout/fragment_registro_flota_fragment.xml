<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragments_Admin.registro_flota_fragment">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="244dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="46dp"
        android:layout_marginBottom="20dp"
        android:text="DATOS DEL VEHICULO"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/etAñoFab"
        app:layout_constraintStart_toStartOf="@+id/etAñoFab"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etAñoFab"
        android:layout_width="290dp"
        android:layout_height="0dp"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="19dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etMarca"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/etMarca"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="23dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etModelo"
        app:layout_constraintEnd_toEndOf="@+id/etModelo"
        app:layout_constraintStart_toStartOf="@+id/etAñoFab"
        app:layout_constraintTop_toBottomOf="@+id/etAñoFab" />

    <EditText
        android:id="@+id/etModelo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="26dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etColor"
        app:layout_constraintEnd_toEndOf="@+id/etAñoFab"
        app:layout_constraintStart_toStartOf="@+id/etAñoFab"
        app:layout_constraintTop_toBottomOf="@+id/etMarca" />

    <EditText
        android:id="@+id/etColor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="26dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etCantAsientos"
        app:layout_constraintEnd_toEndOf="@+id/etModelo"
        app:layout_constraintStart_toStartOf="@+id/etModelo"
        app:layout_constraintTop_toBottomOf="@+id/etModelo" />

    <EditText
        android:id="@+id/etCantAsientos"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/btnRegistrarV"
        app:layout_constraintEnd_toEndOf="@+id/etColor"
        app:layout_constraintStart_toStartOf="@+id/etModelo"
        app:layout_constraintTop_toBottomOf="@+id/etColor" />

    <Button
        android:id="@+id/btnRegistrarV"
        android:layout_width="145dp"
        android:layout_height="0dp"
        android:layout_marginStart="59dp"
        android:layout_marginBottom="206dp"
        android:text="REGISTRAR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/etCantAsientos"
        app:layout_constraintTop_toBottomOf="@+id/etCantAsientos" />
</androidx.constraintlayout.widget.ConstraintLayout>